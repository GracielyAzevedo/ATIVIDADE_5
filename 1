programa {
	funcao inicio() {
		
		real primeiroNumero
		real segundoNumero
		
		escreva("Diga número: ")
		leia(primeiroNumero)
		
		escreva("Diga outro número: ")
		leia(segundoNumero)
		
		se( primeiroNumero > segundoNumero){
	    escreva(primeiroNumero, " é maior que ", segundoNumero)}
	    
		    senao se (primeiroNumero < segundoNumero){
		    escreva(segundoNumero, " é maior que ", primeiroNumero)}
		        
		        senao { escreva("Os números são iguais")
		        
	     }
	}
}
